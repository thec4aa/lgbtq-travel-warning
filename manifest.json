{
  "manifest_version": 3,
  "name": "LGBTQ Travel Warning",
  "version": "1.0",
  "description": "Alerts you when booking travel to locations with dangerously backwards LGBTQ+ laws like Uzbekistan, Uganda, Russia, and Florida.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*.booking.com/*",
        "https://*.kayak.com/*",
        "https://*.kayak.fr/*",
        "https://www.google.com/travel/*",
        "https://*.airbnb.com/*",
        "https://*.vrbo.com/",
        "https://*.priceline.com/*"
      ]
    }
  ]
}
